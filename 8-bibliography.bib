@article{lfunstable,
author = {Rider W. J. and Lowrie R. B.},
title = {The use of classical Lax–Friedrichs Riemann solvers with discontinuous Galerkin methods},
journal = {International Journal for Numerical Methods in Fluids},
volume = {40},
number = {334},
pages = {479-486},
keywords = {discontinuous Galerkin, Lax–Friedrichs, von Neumann stability analysis},
doi = {10.1002/fld.334},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/fld.334},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/fld.334},
abstract = {Abstract While conducting a von Neumann stability analysis of discontinuous Galerkin methods we discovered that the classic Lax–Friedrichs Riemann solver is unstable for all time?step sizes. We describe a simple modification of the Riemann solver's dissipation returns the method to stability. Furthermore, the method has a smaller truncation error than the corresponding method with an upwind flux for the RK2?DG(1) method. These results are verified upon testing. Copyright © 2002 John Wiley \& Sons, Ltd.}
}

